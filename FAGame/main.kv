<G>:
	Game:
		name:'Game'
<Game>:
	FloatLayout:
		id:float
		canvas:
			Color:
				rgba:0.88, 0.13, 0.99, 1
			Rectangle:
				size:self.size
				pos:self.pos
		Contador:
			id:conta
			size_hint:None, None
			text:'0'
			font_size:('50')
			x:210
			y:700
		Bos:
			id:bos
			allow_stretch:True
			size_hint:None, None
			size:root.height/25, root.height/25
			x:100
			y:100
			canvas:
				Color:
					rgba:0.8, 0.9, 0.4, 1
				Rectangle:
					size:self.size
					pos:self.pos
		Player:
			id:player
			allow_stretch:True
			size_hint:None, None
			size:root.height/20, root.height/20
			x:200
			y:500
			canvas:
				Color:
					rgba:1, 1, 1, 1
				Rectangle:
					size:self.size
					pos:self.pos
					source:'barata.png'
		chao2:
			id:chao2
			allow_stretch:True
			size_hint:None, None
			size:200, 30
			x:0
			y:400
			canvas:
				Color:
					rgba:0.66, 0.94, 0.49, 1
				Rectangle:
					size:self.size
					pos:self.pos
		chao3:
			id:chao3
			allow_stretch:True
			size_hint:None, None
			size:200, 30
			x:350
			y:400
			canvas:
				Color:
					rgba:0.66, 0.94, 0.49, 1
				Rectangle:
					size:self.size
					pos:self.pos
		chao:
			allow_stretch:True
			size_hint:None, None
			size:800, 100
			x:0
			y:0
			canvas:
				Color:
					rgba:0.36, 0.84, 0.69, 1
				Rectangle:
					size:self.size
					pos:self.pos
					source:'Image.jpg'
					
		Button:
			text:'>'
			size_hint:None, None
			size:150, 90
			x:390
			y:5
			on_press:root.pd()
			on_release:root.pds()
		Button:
			text:'<'
			size_hint:None, None
			size:150, 90
			x:0
			y:5
			on_press:root.pe()
			on_release:root.pes()
		Button:
			text:'+'
			size_hint:None, None
			size:240, 90
			x:150
			y:5
			on_release:root.pulo()